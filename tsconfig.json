{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "inlineSources": true,
    "sourceRoot": "/",
    "moduleResolution": "bundler"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": []
}